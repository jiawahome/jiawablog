<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jiawablog.mapper.MineUserMapper">
    <select id="selectByLoginName" resultType="com.jiawablog.db.User">
        select * from user t where t.login_name = #{loginName}
    </select>
</mapper>